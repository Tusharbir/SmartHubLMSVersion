<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HODFragment.HODAddStudentFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="times"
            tools:context=".HODFragment.HODAddStudentFragment">


            <EditText
                android:id="@+id/signup_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Email"
                android:inputType="text"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.127"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView30"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/signup_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Name"
                android:inputType="text"
                android:textSize="12sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="293dp" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="274dp" />

            <EditText
                android:id="@+id/signup_rollno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Roll"
                android:inputType="number"
                android:textSize="12sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="18dp"
                tools:layout_editor_absoluteY="241dp" />

            <EditText
                android:id="@+id/signup_mobile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Mobile"
                android:inputType="number"
                android:textSize="12sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="378dp" />

            <CheckBox
                android:id="@+id/checkbox_leet"
                android:layout_width="195dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:fontFamily="times"
                android:text="Is LEET Student?"
                android:textSize="12sp"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="418dp" />

            <EditText
                android:id="@+id/signup_leet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Section"
                android:inputType="text"
                android:textSize="12sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="536dp" />

            <EditText
                android:id="@+id/signup_group"
                android:layout_width="161dp"
                android:layout_height="35dp"
                android:background="#79C5C3C3"
                android:ems="10"
                android:fontFamily="times"
                android:hint="Group"
                android:inputType="text"
                android:textSize="12sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="34dp"
                tools:layout_editor_absoluteY="639dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/ssignup_btn"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:background="@drawable/gradient_button"
                android:fontFamily="times"
                android:text="Add Student"
                android:textColor="@color/white"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="90dp"
                tools:layout_editor_absoluteY="713dp" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="times"
                android:text="Student Credentials"
                android:textColor="@color/black"
                android:textSize="22sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.165"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.042" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="77dp"
                android:layout_height="22dp"
                android:text="TextView"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="203dp" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="64dp"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.09"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView22"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                tools:layout_editor_absoluteX="41dp"
                tools:layout_editor_absoluteY="358dp" />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="483dp" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                tools:layout_editor_absoluteX="34dp"
                tools:layout_editor_absoluteY="588dp" />

            <!--    <Spinner-->
            <!--        android:id="@+id/spinner_sbatch"-->
            <!--        android:layout_width="match_parent"-->
            <!--        android:layout_height="wrap_content"-->
            <!--        android:layout_alignParentStart="true"-->
            <!--        app:layout_constraintBottom_toTopOf="@+id/ssignup_btn"-->
            <!--        app:layout_constraintTop_toBottomOf="@+id/signup_group"-->
            <!--        tools:layout_editor_absoluteX="64dp" />-->

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</FrameLayout>